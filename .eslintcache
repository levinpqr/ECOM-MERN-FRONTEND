op: \"1px\" }}\n                                    >\n                                        <Checkbox\n                                            onChange={(e) =>\n                                                onChangeFilters(\"brand\", e)\n                                            }\n                                            className=\"pb-2 pl-4 pr-4\"\n                                            value={i._id}\n                                            name=\"brand\"\n                                            checked={checkedBrands.includes(\n                                                i._id\n                                            )}\n                                        >\n                                            {i.label}\n                                        </Checkbox>\n                                    </div>\n                                ))}\n                            </div>\n                        </Menu.SubMenu>\n\n                        <Menu.SubMenu key=\"6\" title={COLOR_TITLE}>\n                            <div\n                                style={{\n                                    marginTop: \"-10px\",\n                                    maxHeight: \"300px\",\n                                    overflowY: \"auto\",\n                                }}\n                            >\n                                {colors.map((i) => (\n                                    <div\n                                        key={i._id}\n                                        style={{ paddingTop: \"1px\" }}\n                                    >\n                                        <Checkbox\n                                            onChange={(e) =>\n                                                onChangeFilters(\"color\", e)\n                                            }\n                                            className=\"pb-2 pl-4 pr-4\"\n                                            value={i._id}\